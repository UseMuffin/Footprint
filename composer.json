{
    "name": "muffin/footprint",
    "description": "CakePHP plugin to allow passing currently logged in user to model layer",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "muffin",
        "footprint"
    ],
    "homepage": "https://github.com/usemuffin/footprint",
    "license": "MIT",
    "authors": [
        {
            "name": "Jad Bitar",
            "homepage": "http://jadb.io",
            "role": "Author"
        },
        {
            "name": "ADmad",
            "homepage": "https://github.com/ADmad",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/usemuffin/footprint/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/usemuffin/footprint/issues",
        "source": "https://github.com/usemuffin/footprint"
    },
    "require": {
        "cakephp/cakephp": "^5.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^5.0",
        "phpunit/phpunit": "^10.1"
    },
    "scripts": {
        "cs-check": "phpcs --colors --parallel=16 -p src/ tests/",
        "cs-fix": "phpcbf --colors --parallel=16 -p src/ tests/"
    },
    "autoload": {
        "psr-4": {
            "Muffin\\Footprint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muffin\\Footprint\\Test\\": "tests",
            "TestApp\\": "tests/test_app/TestApp",
            "Cake\\Test\\Fixture\\": "vendor/cakephp/cakephp/tests/Fixture"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
